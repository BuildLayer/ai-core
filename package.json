{
  "name": "@buildlayer/ai-core",
  "version": "0.1.2",
  "description": "Headless AI chat engine and store",
  "keywords": [
    "ai",
    "chat",
    "openai",
    "anthropic",
    "mistral",
    "grok",
    "llm",
    "chatbot",
    "ai-engine",
    "chat-store",
    "tetherai",
    "buildlayer",
    "ai-ui",
    "machine-learning",
    "typescript",
    "javascript",
    "sdk",
    "api",
    "conversation",
    "streaming",
    "tools",
    "adapters"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BuildLayer/ai-core"
  },
  "homepage": "https://buildlayer.dev/ai-ui",
  "bugs": {
    "url": "https://github.com/BuildLayer/ai-core/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@tetherai/anthropic": "^0.4.1",
    "@tetherai/grok": "^0.4.1",
    "@tetherai/local": "^0.4.1",
    "@tetherai/mistral": "^0.4.1",
    "@tetherai/openai": "^0.4.1",
    "nanoid": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  }
}
